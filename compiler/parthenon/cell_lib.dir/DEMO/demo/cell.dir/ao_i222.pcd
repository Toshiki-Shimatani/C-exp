(def-module ao_i222 power 17  area 1.6 gates 6
  
    (def-pin a1 type input load 0.13)
    (def-pin a2 type input load 0.12)
    (def-pin b1 type input load 0.12)
    (def-pin b2 type input load 0.12)
    (def-pin c1 type input load 0.12)
    (def-pin c2 type input load 0.13)
    (def-pin zn type output load 0.087)

    (def-symbol AOI222 a1 a2 b1 b2 c1 c2 zn)

    (def-function ^zn (or (and a1 a2) (and b1 b2) (and c1 c2)))

    (def-delay /a1 /zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay /b1 /zn (+ 0.92 (* 1.8 ~zn)))
    (def-delay /b2 /zn (+ 0.92 (* 1.8 ~zn)))
    (def-delay /c1 /zn (+ 0.97 (* 1.8 ~zn)))
    (def-delay /c2 /zn (+ 0.97 (* 1.8 ~zn)))

    (def-delay %a1 %zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay %b1 %zn (+ 0.92 (* 1.8 ~zn)))
    (def-delay %b2 %zn (+ 0.92 (* 1.8 ~zn)))
    (def-delay %c1 %zn (+ 0.97 (* 1.8 ~zn)))
    (def-delay %c2 %zn (+ 0.97 (* 1.8 ~zn)))

    (def-constraint drive (- 1.1 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
