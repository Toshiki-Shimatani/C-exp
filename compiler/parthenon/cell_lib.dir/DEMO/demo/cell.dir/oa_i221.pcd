(def-module oa_i221 power 13 area 1.1 gates 5
  
    (def-pin a1 type input load 0.11)
    (def-pin a2 type input load 0.11)
    (def-pin b1 type input load 0.12)
    (def-pin b2 type input load 0.12)
    (def-pin c type input load 0.12)
    (def-pin zn type output load 0.068)

    (def-symbol OAI221 a1 a2 b1 b2 c zn)

    (def-function ^zn (and (or a1 a2) (or b1 b2) c))

    (def-delay /a1 /zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay /a2 /zn (+ 1.3 (* 1.7 ~zn)))
    (def-delay /b1 /zn (+ 1.1 (* 1.7 ~zn)))
    (def-delay /b2 /zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay /c  /zn (+ 1.2 (* 1.7 ~zn)))

    (def-delay %a1 %zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay %a2 %zn (+ 1.3 (* 1.7 ~zn)))
    (def-delay %b1 %zn (+ 1.1 (* 1.7 ~zn)))
    (def-delay %b2 %zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay %c  %zn (+ 1.2 (* 1.7 ~zn)))

    (def-constraint drive (- 1.2 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
