(def-module xo3_i2 power 21 area 1.7 gates 8
  
    (def-pin a1 type input load 0.29)
    (def-pin a2 type input load 0.29)
    (def-pin z type output load 0.15)

    (def-symbol EXOR a1 a2 z)

    (def-function z (eor a1 a2))

    (def-delay /a1 /z (+ 1.0 (* 0.60 ~z)))
    (def-delay /a2 /z (+ 1.0 (* 0.60 ~z)))
    (def-delay %a1 %z (+ 1.0 (* 0.60 ~z)))
    (def-delay %a2 %z (+ 1.0 (* 0.60 ~z)))

	(def-constraint drive (- 3.4 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
