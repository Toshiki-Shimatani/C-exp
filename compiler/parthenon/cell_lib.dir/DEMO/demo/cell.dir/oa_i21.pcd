(def-module oa_i21 power 7.8 area 0.86 gates 3
  
    (def-pin a1 type input load 0.12)
    (def-pin a2 type input load 0.12)
    (def-pin b type input load 0.12)
    (def-pin zn type output load 0.072)

    (def-symbol OAI21 a1 a2 b zn)

    (def-function ^zn (and (or a1 a2) b))

    (def-delay /a1 /zn (+ 0.93 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 0.98 (* 1.8 ~zn)))
    (def-delay /b  /zn (+ 0.94 (* 1.8 ~zn)))

    (def-delay %a1 %zn (+ 0.93 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 0.98 (* 1.8 ~zn)))
    (def-delay %b  %zn (+ 0.94 (* 1.8 ~zn)))

    (def-constraint drive (- 1.1 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
