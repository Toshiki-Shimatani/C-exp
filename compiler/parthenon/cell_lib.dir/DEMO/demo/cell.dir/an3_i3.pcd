(def-module an3_i3 power 14 area 1.2 gates 6
  
    (def-pin a1 type input load 0.27)
    (def-pin a2 type input load 0.27)
    (def-pin a3 type input load 0.27)
    (def-pin z type output load 0.14)

    (def-symbol AND a1 a2 a3 z)

    (def-function z (and a1 a2 a3))

    (def-delay /a1 /z (+ 0.76 (* 0.59 ~z)))
    (def-delay /a2 /z (+ 0.76 (* 0.59 ~z)))
    (def-delay /a3 /z (+ 0.76 (* 0.59 ~z)))
    (def-delay %a1 %z (+ 0.76 (* 0.59 ~z)))
    (def-delay %a2 %z (+ 0.76 (* 0.59 ~z)))
    (def-delay %a3 %z (+ 0.76 (* 0.59 ~z)))

	(def-constraint drive (- 3.4 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
