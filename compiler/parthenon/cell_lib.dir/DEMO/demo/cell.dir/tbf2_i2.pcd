(def-module tbf2_i2 power 19 area 0.77 gates 5 

	(def-pin i   type input    load 0.048)
	(def-pin enb type input    load 0.17 note clock)
	(def-pin z   type tristate load 0.099)

	(def-symbol TRI_STATE2 enb i z)

	(def-function z (ts_buf- ^enb i))

	(def-delay /i   /z (+ 0.66 (* 1.0 ~z)))
	(def-delay \enb /z (+ 0.33 (* 1.0 ~z)))
	(def-delay /enb %z 0.34)

	(def-constraint drive (- 2.0 ~z) type max)
	(def-constraint drive (- ~z   0 ) type min)
)
