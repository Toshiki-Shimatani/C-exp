(def-module oa_i31 power  11 area 1.1 gates 4
  
    (def-pin a1 type input load 0.12)
    (def-pin a2 type input load 0.11)
    (def-pin a3 type input load 0.11)
    (def-pin b type input load 0.11)
    (def-pin zn type output load 0.064)

    (def-symbol OAI31 a1 a2 a3 b zn)

    (def-function ^zn (and (or a1 a2 a3) b))

    (def-delay /a1 /zn (+ 1.7 (* 1.9 ~zn)))
    (def-delay /a2 /zn (+ 1.8 (* 1.9 ~zn)))
    (def-delay /a3 /zn (+ 1.8 (* 1.9 ~zn)))
    (def-delay /b  /zn (+ 1.2 (* 1.9 ~zn)))

    (def-delay %a1 %zn (+ 1.7 (* 1.9 ~zn)))
    (def-delay %a2 %zn (+ 1.8 (* 1.9 ~zn)))
    (def-delay %a3 %zn (+ 1.8 (* 1.9 ~zn)))
    (def-delay %b  %zn (+ 1.2 (* 1.9 ~zn)))

    (def-constraint drive (- 1.1 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
