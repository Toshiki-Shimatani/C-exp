(def-module oa_i22 power 12  area 0.98 gates 4
  
    (def-pin a1 type input load 0.11)
    (def-pin a2 type input load 0.11)
    (def-pin b1 type input load 0.11)
    (def-pin b2 type input load 0.11)
    (def-pin zn type output load 0.069)

    (def-symbol OAI22 a1 a2 b1 b2 zn)

    (def-function ^zn (and (or a1 a2) (or b1 b2)))

    (def-delay /a1 /zn (+ 1.1 (* 1.7 ~zn)))
    (def-delay /a2 /zn (+ 1.3 (* 1.7 ~zn)))
    (def-delay /b1 /zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay /b2 /zn (+ 1.3 (* 1.7 ~zn)))

    (def-delay %a1 %zn (+ 1.1 (* 1.7 ~zn)))
    (def-delay %a2 %zn (+ 1.3 (* 1.7 ~zn)))
    (def-delay %b1 %zn (+ 1.2 (* 1.7 ~zn)))
    (def-delay %b2 %zn (+ 1.3 (* 1.7 ~zn)))

    (def-constraint drive (- 1.2 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
