(def-module nr1_i4 power 11 area 0.98 gates 4
  
    (def-pin a1 type input load 0.11)
    (def-pin a2 type input load 0.11)
    (def-pin a3 type input load 0.11)
    (def-pin a4 type input load 0.11)
    (def-pin zn type output load 0.080)

    (def-symbol NOR a1 a2 a3 a4 zn)

    (def-function ^zn (or a1 a2 a3 a4))
   
    (def-delay /a1 /zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay /a3 /zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay /a4 /zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay %a1 %zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay %a3 %zn (+ 1.2 (* 1.8 ~zn)))
    (def-delay %a4 %zn (+ 1.2 (* 1.8 ~zn)))

	(def-constraint drive (- 1.1 ~zn) type max)
	(def-constraint drive (- ~zn 0) type min)
)
