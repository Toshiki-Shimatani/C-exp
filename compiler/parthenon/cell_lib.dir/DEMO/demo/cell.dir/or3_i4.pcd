(def-module or3_i4 power 26 area 2.0 gates 8
  
    (def-pin a1 type input load 0.25)
    (def-pin a2 type input load 0.25)
    (def-pin a3 type input load 0.25)
    (def-pin a4 type input load 0.25)
    (def-pin z type output load 0.21)

    (def-symbol OR a1 a2 a3 a4 z)

    (def-function z (or a1 a2 a3 a4))

    (def-delay /a1 /z (+ 0.90 (* 0.51 ~z)))
    (def-delay /a2 /z (+ 0.90 (* 0.51 ~z)))
    (def-delay /a3 /z (+ 0.90 (* 0.51 ~z)))
    (def-delay /a4 /z (+ 0.90 (* 0.51 ~z)))
    (def-delay %a1 %z (+ 0.90 (* 0.51 ~z)))
    (def-delay %a2 %z (+ 0.90 (* 0.51 ~z)))
    (def-delay %a3 %z (+ 0.90 (* 0.51 ~z)))
    (def-delay %a4 %z (+ 0.90 (* 0.51 ~z)))

	(def-constraint drive (- 3.9 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
