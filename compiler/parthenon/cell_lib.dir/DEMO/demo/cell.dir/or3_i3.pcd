(def-module or3_i3 power 25 area 1.7 gates 8
  
    (def-pin a1 type input load 0.26)
    (def-pin a2 type input load 0.26)
    (def-pin a3 type input load 0.26)
    (def-pin z type output load 0.20)

    (def-symbol OR a1 a2 a3 z)

    (def-function z (or a1 a2 a3))

    (def-delay /a1 /z (+ 0.67 (* 0.50 ~z)))
    (def-delay /a2 /z (+ 0.67 (* 0.50 ~z)))
    (def-delay /a3 /z (+ 0.67 (* 0.50 ~z)))
    (def-delay %a1 %z (+ 0.67 (* 0.50 ~z)))
    (def-delay %a2 %z (+ 0.67 (* 0.50 ~z)))
    (def-delay %a3 %z (+ 0.67 (* 0.50 ~z)))

	(def-constraint drive (- 4.0 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
