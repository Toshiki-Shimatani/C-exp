(def-module nr3_i7 power 54 area 3.9 gates 16
  
    (def-pin a1 type input load 0.25)
    (def-pin a2 type input load 0.25)
    (def-pin a3 type input load 0.25)
    (def-pin a4 type input load 0.25)
    (def-pin a5 type input load 0.25)
    (def-pin a6 type input load 0.25)
    (def-pin a7 type input load 0.25)
    (def-pin zn type output load 0.17)

    (def-symbol NOR a1 a2 a3 a4 a5 a6 a7 zn)

    (def-function ^zn (or a1 a2 a3 a4 a5 a6 a7))

    (def-delay /a1 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a2 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a3 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a4 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a5 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a6 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay /a7 /zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a1 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a2 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a3 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a4 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a5 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a6 %zn (+ 1.7 (* 0.62 ~zn)))
    (def-delay %a7 %zn (+ 1.7 (* 0.62 ~zn)))

	(def-constraint drive (- 3.2 ~zn) type max)
	(def-constraint drive (- ~zn 0) type min)
)
