(def-module xo1_i2 power 10 area 0.86 gates 4
  
    (def-pin a1 type input load 0.15)
    (def-pin a2 type input load 0.15)
    (def-pin z type output load 0.074)

    (def-symbol EXOR a1 a2 z)

    (def-function z (eor a1 a2))

    (def-delay /a1 /z (+ 1.0 (* 1.8 ~z)))
    (def-delay /a2 /z (+ 1.0 (* 1.8 ~z)))
    (def-delay %a1 %z (+ 1.0 (* 1.8 ~z)))
    (def-delay %a2 %z (+ 1.0 (* 1.8 ~z)))

	(def-constraint drive (- 1.1 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
