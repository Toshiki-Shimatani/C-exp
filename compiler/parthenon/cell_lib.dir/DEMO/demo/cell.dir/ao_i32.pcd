(def-module ao_i32 power 13  area 1.2 gates 5
  
    (def-pin a1 type input load 0.13)
    (def-pin a2 type input load 0.13)
    (def-pin a3 type input load 0.13)
    (def-pin b1 type input load 0.12)
    (def-pin b2 type input load 0.12)
    (def-pin zn type output load 0.089)

    (def-symbol AOI32 a1 a2 a3 b1 b2 zn)

    (def-function ^zn (or (and a1 a2 a3)(and b1 b2)))

    (def-delay /a1 /zn (+ 0.75 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay /a3 /zn (+ 0.90 (* 1.8 ~zn)))
    (def-delay /b1 /zn (+ 0.74 (* 1.8 ~zn)))
    (def-delay /b2 /zn (+ 0.74 (* 1.8 ~zn)))

    (def-delay %a1 %zn (+ 0.75 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 0.83 (* 1.8 ~zn)))
    (def-delay %a3 %zn (+ 0.90 (* 1.8 ~zn)))
    (def-delay %b1 %zn (+ 0.74 (* 1.8 ~zn)))
    (def-delay %b2 %zn (+ 0.74 (* 1.8 ~zn)))


    (def-constraint drive (- 1.1 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
