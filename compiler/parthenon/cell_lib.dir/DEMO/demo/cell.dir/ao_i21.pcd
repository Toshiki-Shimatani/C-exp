(def-module ao_i21 power 8.0 area 0.86 gates 3
  
    (def-pin a1 type input load 0.11)
    (def-pin a2 type input load 0.11)
    (def-pin b type input load 0.11)
    (def-pin zn type output load 0.068)

    (def-symbol AOI21 a1 a2 b zn)

    (def-function ^zn (or (and a1 a2) b))

    (def-delay /a1 /zn (+ 0.87 (* 1.7 ~zn)))
    (def-delay /a2 /zn (+ 0.92 (* 1.7 ~zn)))
    (def-delay /b  /zn (+ 1.0  (* 1.7 ~zn)))

    (def-delay %a1 %zn (+ 0.87 (* 1.7 ~zn)))
    (def-delay %a2 %zn (+ 0.92 (* 1.7 ~zn)))
    (def-delay %b  %zn (+ 1.0  (* 1.7 ~zn)))

    (def-constraint drive (- 1.2 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
