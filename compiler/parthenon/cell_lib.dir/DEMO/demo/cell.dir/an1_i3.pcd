(def-module an1_i3 power 7.0 area 0.61 gates 3
  
    (def-pin a1 type input load 0.13)
    (def-pin a2 type input load 0.13)
    (def-pin a3 type input load 0.13)
    (def-pin z type output load 0.068)

    (def-symbol AND a1 a2 a3 z)

    (def-function z (and a1 a2 a3))

    (def-delay /a1 /z (+ 0.76 (* 1.8 ~z)))
    (def-delay /a2 /z (+ 0.76 (* 1.8 ~z)))
    (def-delay /a3 /z (+ 0.76 (* 1.8 ~z)))
    (def-delay %a1 %z (+ 0.76 (* 1.8 ~z)))
    (def-delay %a2 %z (+ 0.76 (* 1.8 ~z)))
    (def-delay %a3 %z (+ 0.76 (* 1.8 ~z)))

	(def-constraint drive (- 1.1 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
