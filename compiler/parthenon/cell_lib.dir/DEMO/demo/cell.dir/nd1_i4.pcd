(def-module nd1_i4 power 6.4 area 0.98 gates 4
  
    (def-pin a1 type input load 0.17)
    (def-pin a2 type input load 0.17)
    (def-pin a3 type input load 0.17)
    (def-pin a4 type input load 0.17)
    (def-pin zn type output load 0.13)

    (def-symbol NAND a1 a2 a3 a4 zn)

    (def-function ^zn (and a1 a2 a3 a4))

    (def-delay /a1 /zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay /a2 /zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay /a3 /zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay /a4 /zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay %a1 %zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay %a2 %zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay %a3 %zn (+ 0.30 (* 1.9 ~zn)))
    (def-delay %a4 %zn (+ 0.30 (* 1.9 ~zn)))

	(def-constraint drive (- 1.0 ~zn) type max)
	(def-constraint drive (- ~zn 0) type min)
)
