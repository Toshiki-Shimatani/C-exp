(def-module nr3_i2 power 9.4 area 1.2 gates 4
  
    (def-pin a1 type input load 0.36)
    (def-pin a2 type input load 0.36)
    (def-pin zn type output load 0.21)

    (def-symbol NOR a1 a2 zn)

    (def-function ^zn (or a1 a2))

    (def-delay /a1 /zn (+ 0.26 (* 0.60 ~zn)))
    (def-delay /a2 /zn (+ 0.26 (* 0.60 ~zn)))
    (def-delay %a1 %zn (+ 0.26 (* 0.60 ~zn)))
    (def-delay %a2 %zn (+ 0.26 (* 0.60 ~zn)))

	(def-constraint drive (- 3.4 ~zn) type max)
	(def-constraint drive (- ~zn 0) type min)
)
