(def-module an1_i7 power 15 area 1.4 gates 5
  
    (def-pin a1 type input load 0.12)
    (def-pin a2 type input load 0.12)
    (def-pin a3 type input load 0.12)
    (def-pin a4 type input load 0.12)
    (def-pin a5 type input load 0.12)
    (def-pin a6 type input load 0.12)
    (def-pin a7 type input load 0.12)
    (def-pin z type output load 0.099)

    (def-symbol AND a1 a2 a3 a4 a5 a6 a7 z)

    (def-function z (and a1 a2 a3 a4 a5 a6 a7))

    (def-delay /a1 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a2 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a3 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a4 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a5 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a6 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay /a7 /z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a1 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a2 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a3 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a4 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a5 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a6 %z (+ 1.5 (* 3.0 ~z)))
    (def-delay %a7 %z (+ 1.5 (* 3.0 ~z)))

	(def-constraint drive (- 0.67 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
