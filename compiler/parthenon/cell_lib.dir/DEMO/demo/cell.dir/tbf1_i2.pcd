(def-module tbf1_i2 power 11 area 0.65 gates 3 

	(def-pin i   type input    load 0.038)
	(def-pin enb type input    load 0.093 note clock)
	(def-pin z   type tristate load 0.054)

	(def-symbol TRI_STATE2 enb i z)

	(def-function z (ts_buf- ^enb i))

	(def-delay /i   /z (+ 0.55 (* 2.0 ~z)))
	(def-delay \enb /z (+ 0.31 (* 2.0 ~z)))
	(def-delay /enb %z 0.33)

	(def-constraint drive (- 1.0 ~z) type max)
	(def-constraint drive (- ~z   0 ) type min)
)
