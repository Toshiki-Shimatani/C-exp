(def-module bgnd1_i2 power 3.4 area 0.49 gates 2 cell_name nd1_i2

	(def-pin a1 type input  load 0.16 note clock)
	(def-pin a2 type input  load 0.16)
	(def-pin zn type output load 0.10)

	(def-symbol NAND a1 a2 zn)

	(def-function ^zn (b_gate--2 a1 a2))

	(def-delay /a1 \zn (+ 0.15 (* 1.8 ~zn)))
	(def-delay \a1 /zn (+ 0.15 (* 1.8 ~zn)))

	(def-constraint a1_a2_setup  (- /a1 /a2 0)          type setup)

	(def-constraint ra2_fa1_hold (- (+ /a2 ?cycle) \a1) type hold)
	(def-constraint za2_fa1_hold (- %a2 \a1 0)          type hold)

	(def-constraint drive (- 1.1 ~zn) type max)
	(def-constraint drive (- ~zn  0)   type min)
)
