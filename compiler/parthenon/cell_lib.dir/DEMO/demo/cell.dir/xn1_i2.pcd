(def-module xn1_i2 power 10 area 0.86 gates 4
  
    (def-pin a1 type input load 0.15)
    (def-pin a2 type input load 0.15)
    (def-pin zn type output load 0.074)

    (def-symbol NEXOR a1 a2 zn)

    (def-function ^zn (eor a1 a2))

    (def-delay /a1 /zn (+ 1.0 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 1.0 (* 1.8 ~zn)))
    (def-delay %a1 %zn (+ 1.0 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 1.0 (* 1.8 ~zn)))

    (def-constraint drive (- 1.1 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
