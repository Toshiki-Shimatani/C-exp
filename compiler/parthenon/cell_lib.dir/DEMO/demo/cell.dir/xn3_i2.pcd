(def-module xn3_i2 power 21 area 1.7 gates 8
  
    (def-pin a1 type input load 0.29)
    (def-pin a2 type input load 0.29)
    (def-pin zn type output load 0.15)

    (def-symbol NEXOR a1 a2 zn)

    (def-function ^zn (eor a1 a2))

    (def-delay /a1 /zn (+ 1.0 (* 0.60 ~zn)))
    (def-delay /a2 /zn (+ 1.0 (* 0.60 ~zn)))
    (def-delay %a1 %zn (+ 1.0 (* 0.60 ~zn)))
    (def-delay %a2 %zn (+ 1.0 (* 0.60 ~zn)))

    (def-constraint drive (- 3.4 ~zn) type max)
    (def-constraint drive (- ~zn 0) type min)
)
