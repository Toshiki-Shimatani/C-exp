(def-module nd1_i3 power 4.9 area 0.74 gates 3
  
    (def-pin a1 type input load 0.17)
    (def-pin a2 type input load 0.17)
    (def-pin a3 type input load 0.17)
    (def-pin zn type output load 0.11)

    (def-symbol NAND a1 a2 a3 zn)

    (def-function ^zn (and a1 a2 a3))

    (def-delay /a1 /zn (+ 0.24 (* 1.8 ~zn)))
    (def-delay /a2 /zn (+ 0.24 (* 1.8 ~zn)))
    (def-delay /a3 /zn (+ 0.24 (* 1.8 ~zn)))
    (def-delay %a1 %zn (+ 0.24 (* 1.8 ~zn)))
    (def-delay %a2 %zn (+ 0.24 (* 1.8 ~zn)))
    (def-delay %a3 %zn (+ 0.24 (* 1.8 ~zn)))

	(def-constraint drive (- 1.1 ~zn) type max)
	(def-constraint drive (- ~zn 0) type min)
)
