(def-module or1_i4 power 13 area 0.98 gates 4
  
    (def-pin a1 type input load 0.13)
    (def-pin a2 type input load 0.13)
    (def-pin a3 type input load 0.13)
    (def-pin a4 type input load 0.13)
    (def-pin z type output load 0.11)

    (def-symbol OR a1 a2 a3 a4 z)

    (def-function z (or a1 a2 a3 a4))

    (def-delay /a1 /z (+ 0.90 (* 1.5 ~z)))
    (def-delay /a2 /z (+ 0.90 (* 1.5 ~z)))
    (def-delay /a3 /z (+ 0.90 (* 1.5 ~z)))
    (def-delay /a4 /z (+ 0.90 (* 1.5 ~z)))
    (def-delay %a1 %z (+ 0.90 (* 1.5 ~z)))
    (def-delay %a2 %z (+ 0.90 (* 1.5 ~z)))
    (def-delay %a3 %z (+ 0.90 (* 1.5 ~z)))
    (def-delay %a4 %z (+ 0.90 (* 1.5 ~z)))

	(def-constraint drive (- 1.3 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
