(def-module or3_i2 power 11 area 1.0 gates 4
  
    (def-pin a1 type input load 0.25)
    (def-pin a2 type input load 0.25)
    (def-pin z type output load 0.14)

    (def-symbol OR a1 a2 z)

    (def-function z (or a1 a2))

    (def-delay /a1 /z (+ 0.60 (* 0.61 ~z)))
    (def-delay /a2 /z (+ 0.60 (* 0.61 ~z)))
    (def-delay %a1 %z (+ 0.60 (* 0.61 ~z)))
    (def-delay %a2 %z (+ 0.60 (* 0.61 ~z)))

	(def-constraint drive (- 3.3 ~z) type max)
	(def-constraint drive (- ~z 0) type min)
)
