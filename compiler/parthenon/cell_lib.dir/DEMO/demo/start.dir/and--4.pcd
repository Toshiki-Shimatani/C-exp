(def-module and--4 power 7.7 area 0.74 gates 3

	(def-pin in1 type input  load 0.14)
	(def-pin in2 type input  load 0.14)
	(def-pin in3 type input  load 0.14)
	(def-pin in4 type input  load 0.14)
	(def-pin out type output load 0.069)

        (def-symbol AND in1 in2 in3 in4 out)

	(def-function out (and
		in1
		in2
		in3
		in4
	))

	(def-delay /in1 /out (+ 0.78 (* 1.8 ~out)))
	(def-delay /in2 /out (+ 0.78 (* 1.8 ~out)))
	(def-delay /in3 /out (+ 0.78 (* 1.8 ~out)))
	(def-delay /in4 /out (+ 0.78 (* 1.8 ~out)))
	(def-delay %in1 %out (+ 0.78 (* 1.8 ~out)))
	(def-delay %in2 %out (+ 0.78 (* 1.8 ~out)))
	(def-delay %in3 %out (+ 0.78 (* 1.8 ~out)))
	(def-delay %in4 %out (+ 0.78 (* 1.8 ~out)))

	(def-constraint drive (- 0 ~out) type max)
)
