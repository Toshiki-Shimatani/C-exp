(def-module oai--31 power 11 area 1.1 gates 4

	(def-pin a1   type input  load 0.12)
	(def-pin a2   type input  load 0.11)
	(def-pin a3   type input  load 0.11)
	(def-pin b    type input  load 0.11)
	(def-pin nout type output load 0.064)

	(def-symbol OAI31 a1 a2 a3 b nout)

	(def-function  ^nout (and (or a1 a2 a3) b))

	(def-delay /a1 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /a2 /nout (+ 1.8 (* 1.9 ~nout)))
	(def-delay /a3 /nout (+ 1.8 (* 1.9 ~nout)))
	(def-delay /b  /nout (+ 1.2 (* 1.9 ~nout)))
	(def-delay %a1 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %a2 %nout (+ 1.8 (* 1.9 ~nout)))
	(def-delay %a3 %nout (+ 1.8 (* 1.9 ~nout)))
	(def-delay %b  %nout (+ 1.2 (* 1.9 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
