(def-module aoi--31 power 12 area 1.1 gates 4

	(def-pin a1   type input  load 0.13)
	(def-pin a2   type input  load 0.13)
	(def-pin a3   type input  load 0.13)
	(def-pin b    type input  load 0.12)
	(def-pin nout type output load 0.072)

	(def-symbol AOI31 a1 a2 a3 b nout)

	(def-function  ^nout (or (and a1 a2 a3) b))

	(def-delay /a1 /nout (+ 0.80 (* 1.8 ~nout)))
	(def-delay /a2 /nout (+ 0.83 (* 1.8 ~nout)))
	(def-delay /a3 /nout (+ 0.89 (* 1.8 ~nout)))
	(def-delay /b  /nout (+ 0.74 (* 1.8 ~nout)))
	(def-delay %a1 %nout (+ 0.80 (* 1.8 ~nout)))
	(def-delay %a2 %nout (+ 0.83 (* 1.8 ~nout)))
	(def-delay %a3 %nout (+ 0.89 (* 1.8 ~nout)))
	(def-delay %b  %nout (+ 0.74 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
