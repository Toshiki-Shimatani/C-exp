(def-module oai--222 power 14 area 1.5 gates 6

	(def-pin a1   type input  load 0.12)
	(def-pin a2   type input  load 0.11)
	(def-pin b1   type input  load 0.11)
	(def-pin b2   type input  load 0.11)
	(def-pin c1   type input  load 0.11)
	(def-pin c2   type input  load 0.11)
	(def-pin nout type output load 0.075)

	(def-symbol OAI222 a1 a2 b1 b2 c1 c2 nout)

	(def-function  ^nout (and (or a1 a2) (or b1 b2) (or c1 c2)))

	(def-delay /a1 /nout (+ 1.4 (* 1.8 ~nout)))
	(def-delay /a2 /nout (+ 1.5 (* 1.8 ~nout)))
	(def-delay /b1 /nout (+ 1.6 (* 1.8 ~nout)))
	(def-delay /b2 /nout (+ 1.7 (* 1.8 ~nout)))
	(def-delay /c1 /nout (+ 1.8 (* 1.8 ~nout)))
	(def-delay /c2 /nout (+ 1.8 (* 1.8 ~nout)))
	(def-delay %a1 %nout (+ 1.4 (* 1.8 ~nout)))
	(def-delay %a2 %nout (+ 1.5 (* 1.8 ~nout)))
	(def-delay %b1 %nout (+ 1.6 (* 1.8 ~nout)))
	(def-delay %b2 %nout (+ 1.7 (* 1.8 ~nout)))
	(def-delay %c1 %nout (+ 1.8 (* 1.8 ~nout)))
	(def-delay %c2 %nout (+ 1.8 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
