(def-module nor--3 power 10 area 0.86 gates 4

	(def-pin in1  type input  load 0.12)
	(def-pin in2  type input  load 0.12)
	(def-pin in3  type input  load 0.12)
	(def-pin nout type output load 0.080)

        (def-symbol NOR in1 in2 in3 nout)

	(def-function ^nout (or
		in1
		in2
		in3
	))

	(def-delay /in1 /nout (+ 1.2 (* 1.8 ~nout)))
	(def-delay /in2 /nout (+ 1.2 (* 1.8 ~nout)))
	(def-delay /in3 /nout (+ 1.2 (* 1.8 ~nout)))
	(def-delay %in1 %nout (+ 1.2 (* 1.8 ~nout)))
	(def-delay %in2 %nout (+ 1.2 (* 1.8 ~nout)))
	(def-delay %in3 %nout (+ 1.2 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
