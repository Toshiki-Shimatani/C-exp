(def-module nand--4 power 6.4 area 0.98 gates 4

	(def-pin in1  type input  load 0.17)
	(def-pin in2  type input  load 0.17)
	(def-pin in3  type input  load 0.17)
	(def-pin in4  type input  load 0.17)
	(def-pin nout type output load 0.13)

        (def-symbol NAND in1 in2 in3 in4 nout)

	(def-function ^nout (and
		in1
		in2
		in3
		in4
	))

	(def-delay /in1 /nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay /in2 /nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay /in3 /nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay /in4 /nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay %in1 %nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay %in2 %nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay %in3 %nout (+ 0.30 (* 1.9 ~nout)))
	(def-delay %in4 %nout (+ 0.30 (* 1.9 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
