(def-module or--3 power 12 area 0.86 gates 4

	(def-pin in1 type input  load 0.13)
	(def-pin in2 type input  load 0.13)
	(def-pin in3 type input  load 0.13)
	(def-pin out type output load 0.10)

        (def-symbol OR in1 in2 in3 out)

	(def-function out (or
		in1
		in2
		in3
	))

	(def-delay /in1 /out (+ 0.67 (* 1.5 ~out)))
	(def-delay /in2 /out (+ 0.67 (* 1.5 ~out)))
	(def-delay /in3 /out (+ 0.67 (* 1.5 ~out)))
	(def-delay %in1 %out (+ 0.67 (* 1.5 ~out)))
	(def-delay %in2 %out (+ 0.67 (* 1.5 ~out)))
	(def-delay %in3 %out (+ 0.67 (* 1.5 ~out)))

	(def-constraint drive (- 0 ~out) type max)
)
