(def-module aoi--21 power 8.0 area 0.86 gates 3

	(def-pin a1   type input  load 0.11)
	(def-pin a2   type input  load 0.11)
	(def-pin b    type input  load 0.11)
	(def-pin nout type output load 0.068)

	(def-symbol AOI21 a1 a2 b nout)

	(def-function  ^nout (or (and a1 a2) b))

	(def-delay /a1 /nout (+ 0.87 (* 1.7 ~nout)))
	(def-delay /a2 /nout (+ 0.92 (* 1.7 ~nout)))
	(def-delay /b  /nout (+ 1.0  (* 1.7 ~nout)))
	(def-delay %a1 %nout (+ 0.87 (* 1.7 ~nout)))
	(def-delay %a2 %nout (+ 0.92 (* 1.7 ~nout)))
	(def-delay %b  %nout (+ 1.0  (* 1.7 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
