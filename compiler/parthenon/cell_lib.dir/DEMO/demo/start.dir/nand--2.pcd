(def-module nand--2 power 3.4 area 0.49 gates 2

	(def-pin in1  type input  load 0.16)
	(def-pin in2  type input  load 0.16)
	(def-pin nout type output load 0.10)

        (def-symbol NAND in1 in2 nout)

	(def-function ^nout (and
		in1
		in2
	))

	(def-delay /in1 /nout (+ 0.15 (* 1.8 ~nout)))
	(def-delay /in2 /nout (+ 0.15 (* 1.8 ~nout)))
	(def-delay %in1 %nout (+ 0.15 (* 1.8 ~nout)))
	(def-delay %in2 %nout (+ 0.15 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
