(def-module or--2 power 5.6 area 0.49 gates 2

	(def-pin in1 type input  load 0.13)
	(def-pin in2 type input  load 0.13)
	(def-pin out type output load 0.068)

        (def-symbol OR in1 in2 out)

	(def-function out (or
		in1
		in2
	))

	(def-delay /in1 /out (+ 0.60 (* 1.8 ~out)))
	(def-delay /in2 /out (+ 0.60 (* 1.8 ~out)))
	(def-delay %in1 %out (+ 0.60 (* 1.8 ~out)))
	(def-delay %in2 %out (+ 0.60 (* 1.8 ~out)))

	(def-constraint drive (- 0 ~out) type max)
)
