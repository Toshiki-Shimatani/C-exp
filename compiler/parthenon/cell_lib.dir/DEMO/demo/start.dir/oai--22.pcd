(def-module oai--22 power 12 area 0.98 gates 4

	(def-pin a1   type input  load 0.11)
	(def-pin a2   type input  load 0.11)
	(def-pin b1   type input  load 0.11)
	(def-pin b2   type input  load 0.11)
	(def-pin nout type output load 0.069)

	(def-symbol OAI22 a1 a2 b1 b2 nout)

	(def-function  ^nout (and (or a1 a2) (or b1 b2)))

	(def-delay /a1 /nout (+ 1.1 (* 1.7 ~nout)))
	(def-delay /a2 /nout (+ 1.3 (* 1.7 ~nout)))
	(def-delay /b1 /nout (+ 1.2 (* 1.7 ~nout)))
	(def-delay /b2 /nout (+ 1.3 (* 1.7 ~nout)))
	(def-delay %a1 %nout (+ 1.1 (* 1.7 ~nout)))
	(def-delay %a2 %nout (+ 1.3 (* 1.7 ~nout)))
	(def-delay %b1 %nout (+ 1.2 (* 1.7 ~nout)))
	(def-delay %b2 %nout (+ 1.3 (* 1.7 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
