(def-module oai--21 power 7.8 area 0.86 gates 3

	(def-pin a1   type input  load 0.12)
	(def-pin a2   type input  load 0.12)
	(def-pin b    type input  load 0.12)
	(def-pin nout type output load 0.072)

	(def-symbol OAI21 a1 a2 b nout)

	(def-function  ^nout (and (or a1 a2) b))

	(def-delay /a1 /nout (+ 0.93 (* 1.8 ~nout)))
	(def-delay /a2 /nout (+ 0.98 (* 1.8 ~nout)))
	(def-delay /b  /nout (+ 0.94 (* 1.8 ~nout)))
	(def-delay %a1 %nout (+ 0.93 (* 1.8 ~nout)))
	(def-delay %a2 %nout (+ 0.98 (* 1.8 ~nout)))
	(def-delay %b  %nout (+ 0.94 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
