(def-module oai--211 power 11 area 0.98 gates 4

	(def-pin a1   type input  load 0.11)
	(def-pin a2   type input  load 0.11)
	(def-pin b    type input  load 0.11)
	(def-pin c    type input  load 0.11)
	(def-pin nout type output load 0.068)

	(def-symbol OAI211 a1 a2 b c nout)

	(def-function  ^nout (and (or a1 a2) b c))

	(def-delay /a1 /nout (+ 1.2  (* 1.7 ~nout)))
	(def-delay /a2 /nout (+ 1.3  (* 1.8 ~nout)))
	(def-delay /b  /nout (+ 0.99 (* 1.8 ~nout)))
	(def-delay /c  /nout (+ 0.99 (* 1.8 ~nout)))
	(def-delay %a1 %nout (+ 1.2  (* 1.7 ~nout)))
	(def-delay %a2 %nout (+ 1.3  (* 1.8 ~nout)))
	(def-delay %b  %nout (+ 0.99 (* 1.8 ~nout)))
	(def-delay %c  %nout (+ 0.99 (* 1.8 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
