(def-module and--7 power 15 area 1.4 gates 5

	(def-pin in1 type input  load 0.12)
	(def-pin in2 type input  load 0.12)
	(def-pin in3 type input  load 0.12)
	(def-pin in4 type input  load 0.12)
	(def-pin in5 type input  load 0.12)
	(def-pin in6 type input  load 0.12)
	(def-pin in7 type input  load 0.12)
	(def-pin out type output load 0.099)

        (def-symbol AND in1 in2 in3 in4 in5 in6 in7 out)

	(def-function out (and
		in1
		in2
		in3
		in4
		in5
		in6
		in7
	))

	(def-delay /in1 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in2 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in3 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in4 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in5 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in6 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay /in7 /out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in1 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in2 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in3 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in4 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in5 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in6 %out (+ 1.5 (* 3.0 ~out)))
	(def-delay %in7 %out (+ 1.5 (* 3.0 ~out)))

	(def-constraint drive (- 0 ~out) type max)
)
