(def-module ts_buf- power 11 area 0.65 gates 3 

	(def-pin in   type input    load 0.038)
	(def-pin nenb type input    load 0.093 note clock)
	(def-pin out  type tristate load 0.054)

        (def-symbol TRI_STATE2 nenb in out)

	(def-function out (ts_buf- ^nenb in))

	(def-delay /in   /out (+ 0.55 (* 2.0 ~out)))
	(def-delay \nenb /out (+ 0.31 (* 2.0 ~out)))
	(def-delay /nenb %out 0.33)

	(def-constraint drive (- 0 ~out) type max)
)
