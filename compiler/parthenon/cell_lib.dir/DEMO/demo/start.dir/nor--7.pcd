(def-module nor--7 power 27 area 2.0 gates 8

	(def-pin in1  type input  load 0.13)
	(def-pin in2  type input  load 0.13)
	(def-pin in3  type input  load 0.13)
	(def-pin in4  type input  load 0.13)
	(def-pin in5  type input  load 0.13)
	(def-pin in6  type input  load 0.13)
	(def-pin in7  type input  load 0.13)
	(def-pin nout type output load 0.086)

        (def-symbol NOR in1 in2 in3 in4 in5 in6 in7 nout)

	(def-function ^nout (or
		in1
		in2
		in3
		in4
		in5
		in6
		in7
	))

	(def-delay /in1 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in2 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in3 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in4 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in5 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in6 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay /in7 /nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in1 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in2 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in3 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in4 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in5 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in6 %nout (+ 1.7 (* 1.9 ~nout)))
	(def-delay %in7 %nout (+ 1.7 (* 1.9 ~nout)))

	(def-constraint drive (- 0 ~nout) type max)
)
